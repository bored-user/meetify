{
    "manifest_version": 2,
    "name": "Meetify",
    "version": "1.0",
    "description": "Automatically switch to Google's school account when entering a Meet reunion.",
    "icons": {
        "48": "icons/meet-48.png",
        "96": "icons/meet-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://meet.google.com/*"
            ],
            "js": [
                "thirdParty/jquery-3.5.1.slim.min.js",
                "meetify.js"
            ]
        }
    ]
}
